    "def luy_thua(a, n):\n",
    "    x = 1\n",
    "    while n != 0:\n",
    "        if n % 2 ==1:\n",
    "            x = x * a\n",
    "        a = a *a\n",
    "        n = n // 2\n",
    "    return x"
